@{
    ViewBag.Title = "修改密码";
    Layout = "~/_LAYOUTS/Master/Pop.cshtml";
}
<script type="text/javascript" charset="utf-8" src="/_layouts/js/home/changepwd.js"></script>

<form role="form">
    <div class="form-body">

        <div class="form-group">
            <label class="control-label col-md-3" for="inputWarning">原密码</label>
            <div class="col-md-6">
                <input type="text" class="form-control" id="pwd">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3" for="inputWarning">新密码</label>
            <div class="col-md-6">
                <input type="text" class="form-control" id="pwd1">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3" for="inputWarning">确认密码</label>
            <div class="col-md-6">
                <input type="text" class="form-control" id="pwd2">
            </div>
        </div>
       
    </div>
</form>





